{
  "openapi": "3.0.0",
  "info": {
    "title": "API Teste Sossego",
    "version": "1.0.0"
  },
  "paths": {
    "/users": {
      "post": {
        "summary": "Cadastro de Us√∫ario",
        "tags": ["Usuarios"],
        "security": [{ "bearerAuth": [] }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/User" },
              "examples": {
                "value": {
                  "fullName": "Frederico Rook",
                  "email": "rook@mail.com",
                  "password": "A123456*",
                  "phoneNumber": "31989998888"
                }
              }
            }
          }
        },
        "responses": {
          "401:": {
            "description": "Unauthorized"
          },
          "400:": {
            "description": "User Already Exists"
          },
          "201:": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "$ref": "#/components/schemas/users"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "number"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}
